<config>
    <moduleName>khtml</moduleName>
    <parentModules>
        <module>kde</module>
    </parentModules>
    <scalarTypes>
        <!-- QString is a class, but represented as a scalar (#) in munged names -->
        <typeName>QString</typeName>
    </scalarTypes>
    <voidpTypes>
        <!-- both are classes, but they are represented as Smoke::t_voidp -->
        <typeName>QStringList</typeName>
        <typeName>QString</typeName>
    </voidpTypes>
    <exclude>
        <!-- private stuff -->
        <signature>.*::d</signature>
        <signature>.*::d_ptr</signature>
        <signature>.*::q_ptr</signature>
        <signature>.*::q</signature>
        <signature>.*QDebug.*</signature>
        <signature>.*Q3.*</signature>
        
        <!-- unimplemented public API.. yay -->
        <signature>.*DOM::Element::(set)?(c|C)ontentEditable\(.*</signature>
        <signature>.*DOM::TextEvent::data\(\).*</signature>
    </exclude>
    <functions>
        <!-- everything in that namespace.. -->
        <name>DOM::.*</name>
    </functions>
    <classList>
        <class>DOM</class>
        <class>DOM::AbstractView</class>
